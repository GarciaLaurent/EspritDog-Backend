{
  "name": "@maiia-services/mobile-store-reviews",
  "version": "0.0.1",
  "scripts": {
    "dev": "yarn watch",
    "build": "yarn build-ts && yarn lint",
    "build-ts": "yarn build:clean && tsc --declaration",
    "build:clean": "rimraf dist",
    "lint": "tsc --noEmit && eslint \"**/*.{js,ts}\" --quiet --fix",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"yarn watch-ts\" \"yarn watch-node\"",
    "watch-node": "nodemon dist/server.js",
    "watch-ts": "tsc -w",
    "start": "yarn serve",
    "serve": "node dist/server.js"
  },
  "dependencies": {
    "@slack/webhook": "^6.0.0",
    "app-store-scraper": "^0.16.3",
    "compression": "^1.7.4",
    "errorhandler": "^1.5.1",
    "express": "^4.17.1",
    "firebase-admin": "^9.4.2",
    "google-play-scraper": "^8.0.2",
    "json-concat": "^0.0.1",
    "mv": "^2.1.1",
    "node-cron": "^2.0.3",
    "recursive-diff": "^1.0.8",
    "rimraf": "^3.0.2"
  },
  "devDependencies": {
    "@types/compression": "^1.7.0",
    "@types/errorhandler": "^1.5.0",
    "@types/express": "^4.17.11",
    "@types/mv": "^2.1.0",
    "@types/node-cron": "^2.0.3",
    "@types/rimraf": "^3.0.0"
  }
}
