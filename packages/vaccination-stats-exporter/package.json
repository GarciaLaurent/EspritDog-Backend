{
  "name": "@maiia-services/vaccination-stats-exporter",
  "version": "0.0.1",
  "scripts": {
    "dev": "yarn watch",
    "build": "yarn build-ts && yarn lint",
    "build-ts": "yarn build:clean && tsc --declaration",
    "build:clean": "rimraf dist",
    "lint": "tsc --noEmit && eslint \"**/*.{js,ts}\" --quiet --fix",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"yarn watch-ts\" \"yarn watch-node\"",
    "watch-node": "nodemon dist/index.js",
    "watch-ts": "tsc -w",
    "start": "node dist/index.js"
  },
  "dependencies": {
    "@slack/webhook": "^6.0.0",
    "adm-zip": "^0.5.2",
    "csv-writer": "^1.6.0",
    "dotenv": "^8.2.0",
    "moment": "^2.29.1",
    "mongodb": "^3.6.4",
    "ssh2-sftp-client": "^6.0.1",
    "tmp": "^0.2.1"
  },
  "devDependencies": {
    "@types/adm-zip": "^0.4.33",
    "@types/mongodb": "^3.6.3",
    "@types/ssh2-sftp-client": "^5.3.1",
    "@types/tmp": "^0.2.0"
  }
}